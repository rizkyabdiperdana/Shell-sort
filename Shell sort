#include<iostream>
#include<conio.h>
using namespace std;
int main()
{
  int val[100];
   int i, n, t;

   
   cout<<" \n";
 cout<<" Masukkan Jumlah Data yg ingin di urutkan : ";cin>>n;
cout<<" \n";
for(i = 0; i < n; i++)
{
       cout<<"Nilai ke-"<<1+i<<"  : ";cin>>val[i];
   }

   
    for(int a = n/2; a > 0;a=a/2)
{
       for(int b = a; b < n; b++)
{
          for (int c = b-a; c >= 0; c-=a)
 {
             if(val[c+a] < val[c])
{
                t=val[c+a];
                val[c+a]=val[c];
                val[c]=t;
            }
         }
      }
   }
